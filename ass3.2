#include<stdio.h>
int main()
{
	int a[100],c=0,n,i,j,f[100];
	printf("\nEnter number of terms");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
		f[i]=1;
	}
	 for(i=0; i<n; i++)
    {
        c= 1;
        for(j=i+1;j<n;j++)
        {
            if(a[i]==a[j])
            {
                c++;
                f[j] = 0;
            }
        }
        if(f[i]!=0)
        {
        	f[i]=c;
		}
		printf("\n%d %d",a[i],f[i]);    
		}
}
